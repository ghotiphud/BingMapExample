<Application
    x:Class="BingMapExample.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BingMapExample"
    xmlns:localData="using:BingMapExample.Data">

    <Application.Resources>
        <ResourceDictionary>
            <x:String x:Key="AppName">BingMapExample</x:String>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="SecretDictionary.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
